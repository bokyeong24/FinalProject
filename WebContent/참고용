    function NaverChannelingEndInformationPop() {       
        var xPos = (document.body.clientWidth / 2) - (700 / 2);
        xPos += window.screenLeft;
        var yPos = (screen.availHeight / 2) - (400 / 2);
        var pop = window.open('/News/Popup/2020/NaverChannelingEnd', 'NaverChannelingEndInformation', 'toolbar=no, scrollbars=yes, width=700, height=400, left=' + xPos + ', top=' + yPos);
        if (pop === null) {
            alert('2020년 12월 1일 메이플스토리의 네이버 채널링 서비스가 종료됩니다.\n2020년 10월 30일부터 네이버 채널링 서비스 신규 가입을 하실 수 없습니다.');
        }
    }            
</script>

