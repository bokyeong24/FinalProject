<script>
    $(document).ready(function () {
        $('.word_wrap .word_chk_wrap a').bind('click', function (e) {
            e.preventDefault();
            var category = $(this).siblings("input[name=world_radio]").val();
            var search = encodeURIComponent('c=' + category);
            window.location.href = '/Community/Free?page=1&search=' + search;
        });
    });
</script>

<div class="word_wrap">
    <ul class="word_chk_wrap">
        <!--선택된 선택자 class active 추가-->
